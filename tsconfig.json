{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/components/navbar/hamburgermenu/hamburgermenu.jsx", "src/app/components/navbar/mobile_menu/mobile_menu.jsx", "src/app/cv/page.jsx", "src/app/contact/page.jsx", "src/app/blog/page.jsx", "data/startpage_data.js", "src/app/api/hello/route.js", "src/app/components/buttons.jsx"],
  "exclude": ["node_modules"]
}
